{
  "name": "kintone-vue-component",
  "version": "0.1.8",
  "scripts": {
    "demo": "vue-cli-service serve",
    "build-demo": "vue-cli-service build --target app --dest docs/demo ./src/main.ts",
    "build:type": "tsc --emitDeclarationOnly --declaration -P tsconfig.types.json",
    "build:lib": "vue-cli-service build --target lib --name kintone-vue-component --formats commonjs ./src/components/index.ts",
    "build": "run-s --print-label build:lib build:type",
    "lint": "vue-cli-service lint src"
  },
  "main": "./dist/kintone-vue-component.common.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@mdi/js": "^5.1.45",
    "core-js": "^3.6.4",
    "lodash": "^4.14.154",
    "npm-run-all": "^4.1.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.156",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-typescript": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "~3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
